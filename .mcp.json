{
  "mcpServers": {
    "moodle": {
      "command": "npx",
      "args": [
        "-y",
        "moodle-mcp-server"
      ],
      "env": {
        "MOODLE_API_URL": "${MOODLE_API_URL}",
        "MOODLE_API_TOKEN": "${MOODLE_API_TOKEN}",
        "MOODLE_COURSE_ID": "${MOODLE_COURSE_ID}"
      }
    },
    "aws-api": {
      "command": "uvx",
      "args": [
        "mcp-server-aws"
      ],
      "env": {
        "AWS_PROFILE": "${AWS_PROFILE:-default}",
        "AWS_REGION": "${AWS_REGION:-us-east-1}",
        "ALLOW_WRITE": "${ALLOW_WRITE:-false}"
      }
    }
  }
}